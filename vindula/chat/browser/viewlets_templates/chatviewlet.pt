<div id="online-contacts_viewlet" >
	<tal:def tal:define="member python:context.portal_membership.getAuthenticatedMember()">
		<tal:cond condition="python: (str(member) != 'Anonymous User') and (viewlet.enableChat())">
			<div class="portletWrapper" id="online-contacts" >
				<dl class="portlet online-contacts" i18n:domain="vindula.chat" style="margin-bottom:0;">
			    	
				    	<dt class="portletHeader">
				        	<span >Quem está online?</span>
				    	</dt>
						<dd class="portletItem">
							<input type="hidden" name="username" id="username" tal:attributes="value member/id|nothing" />
							<div id="online-users-container" class="autohide">
					        	<ul id="online-users">
					            	<li id="no-users-online">Não há usuários online</li>
					        	</ul>
					    	</div>
				        </dd>
						<dd class="portletFooter" ></dd>
			
				</dl>
			</div>
		</tal:cond>
	</tal:def>
</div>